pipeline {
	agent any

	stages {
	        stage("Git Clone") {
	            steps {
	                git branch: 'prod', url: 'https://github.com/prj-space-z/telegram-bot.git'
	            }       
	        }
	
	        stage('Start') {
			steps {
				sh 'docker-compose up --build -d'
			}
		} 
	}
}
